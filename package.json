{
  "name": "camo-nano-light-wallet",
  "version": "v1.0.2",
  "description": "camo nano light wallet",
  "productName": "camo-nano-light-wallet",
  "main": "index.js",
  "build": {
    "mac": {
      "target": [
        "dmg"
      ],
      "hardenedRuntime": true
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    }
  },
  "scripts": {
    "start": "check-node-version --node 12 --npm 6 && electron . --enable-logging --remote-debugging-port=9222",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mwl",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w",
    "dist-linux": "electron-builder -l",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coranos/camo-nano-light-wallet.git"
  },
  "keywords": [
    "camo nano wallet"
  ],
  "author": "coranos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coranos/camo-nano-light-wallet/issues"
  },
  "homepage": "https://github.com/coranos/camo-nano-light-wallet#readme",
  "dependencies": {
    "@bananocoin/bananojs": "^2.0.10",
    "@ledgerhq/hw-transport-node-hid": "^5.19.1",
    "babel-polyfill": "^6.26.0",
    "bs58": "^4.0.1",
    "check-node-version": "^4.0.3",
    "electron-store": "^6.0.0",
    "elliptic": "^6.5.3",
    "hw-app-nano": "^1.3.0",
    "node-jsx": "^0.13.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.2.2",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "smart-buffer": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "electron": "^9.3.0",
    "electron-builder": "^22.7.0",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.0.1"
  }
}
